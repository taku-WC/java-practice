package polymorphism.human;

public class Student {
	private int score;

	public Student(String name, int age, int score) {
		super(name, age);
		this.score = score;
	}

	public String getProfile() {
		String profileString = "年齢は" + super.age + "です。";
		profileString += "学生で、テストの点数は" + this.score + "点です。";
		return profile;
	}
}
