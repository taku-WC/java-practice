package capsule.human;

import sun.java2d.marlin.stats.Histogram;

public class HumanCapsule {
	private String name;
	private int age;

	public HumanCapsule(String name, int age) {
		this.name = name;
		this.age = age;

	}

	public static  getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;

	}

	public int getAge() {
		return this.age;
	}
}
