package extend;

public class Interface01 {
	public static void main(String[] args) {

		MobilePhone nobilePhone = new MobilePhone("000-1111-2222");


		System.out.println("電話番後は最大" + MobilePhone.MAX_NUMBER_DIGITS + "桁です。");

		mobilePhone.call("99-8888-7777");
	}

	public static void classTakePicture(Camera camera) {

		camera.takePicture();
	}
}
